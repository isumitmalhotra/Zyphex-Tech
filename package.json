  "private": true,
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 next build",
    "dev": "next dev",
    "dev:custom": "cross-env NODE_ENV=development node server.js",
    "dev:next": "next dev",
    "lint": "next lint",
    "start": "cross-env NODE_ENV=production node server.js",
    "start:next": "next start",
    "type-check": "tsc --noEmit",
    "db:seed": "node prisma/seed.js",
    "generate-types": "tsx scripts/generate-api-types.ts",
    "test:email": "tsx scripts/test-email.ts",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --ci --coverage --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:unit": "jest --testMatch=\"**/__tests__/**/*.test.{ts,tsx}\" --testPathIgnorePatterns=\"integration|api\" --passWithNoTests",
    "test:integration": "jest --testMatch=\"**/__tests__/integration/**/*.test.{ts,tsx}\" --passWithNoTests",
    "test:api": "jest --testMatch=\"**/__tests__/api/**/*.test.{ts,tsx}\" --passWithNoTests",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:chrome": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:mobile": "playwright test --project=Mobile",
    "test:all": "tsx scripts/test-all.ts",
    "test:critical": "jest --testMatch=\"**/__tests__/**/(auth|projects|payments)*.test.{ts,tsx}\" --passWithNoTests && playwright test e2e/auth-flow.spec.ts e2e/project-management.spec.ts e2e/payment-flow.spec.ts",
    "playwright:install": "playwright install",
    "playwright:codegen": "playwright codegen http://localhost:3000",
    "security-audit": "tsx scripts/security-audit.ts",
    "audit:security": "tsx scripts/security-audit.ts",
    "performance-audit": "tsx scripts/performance-audit.ts",
    "audit:performance": "tsx scripts/performance-audit.ts",
    "audit:all": "npm run security-audit && npm run performance-audit",
    "analyze": "ANALYZE=true next build",
    "deploy:staging": "vercel",
    "deploy:production": "powershell -ExecutionPolicy Bypass -File ./scripts/deploy-production.ps1",
    "deploy:production:force": "powershell -ExecutionPolicy Bypass -File ./scripts/deploy-production.ps1 -Force",
    "deploy:vps": "ssh deploy@66.116.199.219 'cd /var/www/zyphextech && ./scripts/deploy-manual.sh'",
    "db:migrate": "npx prisma migrate deploy",
    "db:backup": "tsx scripts/backup-database.ts",
    "db:studio": "npx prisma studio",
    "postinstall": "prisma generate"
  },
